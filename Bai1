public class Main {
    public static void main(String[] args) {
        for (int i = 0;i<4;i++) {
            System.out.println("* * * *");
        }
        System.out.println(" ");
        String sum = "* ";
        for (int i = 0;i<4;i++) {
            System.out.println(sum);
            sum +="* ";
        }
  }
}
